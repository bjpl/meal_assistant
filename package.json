{
  "name": "meal-assistant",
  "version": "1.0.0",
  "description": "Personal Meal System Tracker with 7-pattern flexible eating system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node src/api/server.js",
    "dev": "nodemon src/api/server.js",
    "web": "cd src/mobile && npm run web",
    "mobile": "cd src/mobile && npm start",
    "mobile:android": "cd src/mobile && npm run android",
    "mobile:ios": "cd src/mobile && npm run ios",
    "mobile:install": "cd src/mobile && npm install",
    "test": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --runInBand",
    "test:parallel": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js",
    "test:unit": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --selectProjects unit --runInBand",
    "test:integration": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --selectProjects integration",
    "test:e2e": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --selectProjects e2e",
    "test:performance": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js tests/performance",
    "test:api-heavy": "node --max-old-space-size=8192 node_modules/jest/bin/jest.js --config config/jest.config.js --testPathPattern=apiService.test.ts --runInBand --no-coverage",
    "test:slices": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --testPathPattern=tests/unit/slices --runInBand",
    "test:services": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --testPathPattern=tests/unit/services --runInBand",
    "test:prep": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --testPathPattern=tests/unit/services/prep --runInBand",
    "test:watch": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --watch",
    "test:coverage": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --coverage --runInBand",
    "test:ci": "node --max-old-space-size=4096 node_modules/jest/bin/jest.js --config config/jest.config.js --ci --coverage --reporters=default --reporters=jest-junit --runInBand",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi local",
    "db:migrate": "bash scripts/migrate.sh up",
    "db:rollback": "bash scripts/migrate.sh down",
    "db:status": "bash scripts/migrate.sh status",
    "db:seed": "psql $DATABASE_URL -f scripts/seed-data.sql"
  },
  "keywords": [
    "meal-tracking",
    "nutrition",
    "meal-prep",
    "intermittent-fasting",
    "pwa",
    "kitchen",
    "scheduling"
  ],
  "author": "Brandon",
  "license": "MIT",
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.15.6",
    "@types/react-native": "^0.72.8",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "msw": "^2.12.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@reduxjs/toolkit": "^2.11.0",
    "agentdb": "^1.6.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.12.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "metro": "^0.80.12",
    "metro-resolver": "^0.80.12",
    "morgan": "^1.10.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "react-native-web": "^0.19.13",
    "react-redux": "^9.2.0",
    "sharp": "^0.33.0",
    "string-similarity": "^4.0.4",
    "tesseract.js": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
